{
	"name": "vscode-qasm",
	"displayName": "OpenQASM 3.0 Language Support",
	"description": "Language support for OpenQASM 3.0 (Open Quantum Assembly Language) with syntax highlighting and language server",
	"version": "0.1.3",
	"publisher": "orangekame3",
	"icon": "icons/vscode-qasm.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/orangekame3/qasmtools"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Programming Languages",
		"Formatters",
		"Other"
	],
	"keywords": [
		"qasm",
		"quantum",
		"assembly",
		"quantum computing",
		"openqasm"
	],
	"activationEvents": [
		"onLanguage:qasm"
	],
	"main": "./extension.js",
	"contributes": {
		"languages": [
			{
				"id": "qasm",
				"aliases": [
					"QASM",
					"qasm",
					"OpenQASM"
				],
				"extensions": [
					".qasm"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "qasm",
				"scopeName": "source.qasm",
				"path": "./syntaxes/qasm.tmLanguage.json"
			}
		],
		"semanticTokenTypes": [
			{
				"id": "gate",
				"superType": "function",
				"description": "QASM gates"
			},
			{
				"id": "measurement",
				"superType": "function",
				"description": "QASM measurements"
			},
			{
				"id": "register",
				"superType": "type",
				"description": "QASM registers"
			},
			{
				"id": "builtin_gate",
				"superType": "function",
				"description": "QASM builtin gates"
			},
			{
				"id": "builtin_quantum",
				"superType": "function",
				"description": "QASM builtin quantum operations"
			},
			{
				"id": "builtin_classical",
				"superType": "function",
				"description": "QASM builtin classical functions"
			},
			{
				"id": "builtin_constant",
				"superType": "variable",
				"description": "QASM builtin constants"
			},
			{
				"id": "access_control",
				"superType": "keyword",
				"description": "QASM access control keywords"
			},
			{
				"id": "extern",
				"superType": "keyword",
				"description": "QASM extern keyword"
			},
			{
				"id": "hardware_qubit",
				"superType": "variable",
				"description": "QASM hardware qubits"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node build.js",
		"postinstall": "node build.js",
		"build": "node build.js",
		"package": "npm run build && vsce package",
		"publish": "npm run build && vsce publish"
	},
	"dependencies": {
		"vscode-languageclient": "^8.0.0"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.60.0",
		"typescript": "^4.5.5",
		"@vscode/vsce": "^2.19.0"
	},
	"author": "orangekame3",
	"license": "Apache-2.0",
	"type": "commonjs"
}
